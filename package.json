{
  "name": "json-stringify-extended",
  "version": "0.7.0",
  "description": "JSON.stringify with extended data types support",
  "main": "main.js",
  "engines": {
    "node": ">= 4.x"
  },
  "devDependencies": {
    "babel-cli": "^6.x",
    "babel-preset-es2015": "^6.x",
    "standard": "^x",
    "tap": "^10.x"
  },
  "standard": {
    "ignore": [
      "test/"
    ]
  },
  "scripts": {
    "test": "standard && tap test/*",
    "test-cov": "standard main.js && tap test/* --cov",
    "build": "babel main.js -d dist --presets es2015 --minified --no-comments"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braceslab/json-stringify-extended.git"
  },
  "keywords": [
    "json",
    "stringify"
  ],
  "author": "Simone Sanfratello <simone@braceslab.com>",
  "license": "MIT",
  "dependencies": {
    "uglify-js": "^3.x"
  }
}
